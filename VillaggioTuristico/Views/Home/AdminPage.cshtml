@{
    ViewData["Title"] = "Area riservata amministratore";
}

<div class="container">
    <div class="row">
        <div class="col-sm-12 p-3 text-center">
            <h1>@ViewData["Title"]</h1>
        </div>
        <div class="col-sm-12 p-3 text-center">
            <button class="btn btn-primary btn-lg" onclick="ajaxCallGetAllPrenotations('@Url.Action("GetAllPrenotations", "Home")')">Stampa prenotazioni</button>
        </div>
    </div>
    <div class="row d-flex justify-content-center">
        <div class="col-sm-6">
            <div id="resultDivAdminPage">
                @*popolato da ajaxCallGetAllPrenotations*@
            </div>
        </div>
    </div>
</div>
